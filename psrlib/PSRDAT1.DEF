
C
C     HEADER VARIABLES FOR THE INPUT BLOCK
C
      INTEGER INHEAD(LENHEAD)
      EQUIVALENCE (DATATYPE,INHEAD(1))
C
      COMMON /INHEAD/ LASPDAT,DATATYPE,PULSENO,UTS,DEMRA,DEMDEC,PB
     &             ,PBEPOCH,PBDOT,DELAY,TBIN,DM,SBS,MEANANOM
     &             ,POBS,EPOCH,PDOT,UTF,PA,RA,DEC,LONG,LAT
     &             ,AZ,EL,CLOCK(5),NPULSES,OBSVERS,NBIN
     &             ,NPPH,NCH,NCORIG,DDU,DDUFREQ,CAL,ITELNO
     &             ,INRD,INWR,LENCHAN,LENDAT,COMMENT
C
C     ... CHANNEL DEPENDENT PARAMETERS
C
     &             ,ICCURR,TRES,DCLEV,DCLON,DCLOFF
     &             ,SIGMA,NP,NPON,NPOFF,FOBS,CPOL
C
C TYPE DECLARATIONS
      REAL*8 UTS,UTF,PB,PBEPOCH,POBS,EPOCH,PBIN,EPBIN,CLOCKERR,DDUCORR
      REAL*4 DEMRA,DEMDEC,PBDOT,DELAY,TBIN,DM
     &      ,MEANANOM,PDOT,PA,RA,DEC,AZ,EL
     &      ,LONG,LAT,CLOCK,DDU,DDUFREQ,OBSVERS
      REAL*4 TRES(MAXCHAN),DCLEV(MAXCHAN),DCLON(MAXCHAN)
     &      ,DCLOFF(MAXCHAN),SIGMA(MAXCHAN),FOBS(MAXCHAN)
      INTEGER ICCURR(MAXCHAN),NP(MAXCHAN),NPON(MAXCHAN)
     &      ,NPOFF(MAXCHAN)
      INTEGER DATATYPE,PULSENO,SBS,FLAGS(6)
     &      ,NPULSES,NBIN,NPPH,NCH,NCORIG,ITELNO,INRD,INWR
     &      ,LENCHAN,LENDAT
      LOGICAL CAL,LASPDAT,LARTHURDAT
      CHARACTER COMMENT*160,PSRNAM*8
      CHARACTER*4 CPOL(MAXCHAN)
C
C
C EDTLST CONTAINS NEDT LINES OF EDITTING INSTRUCTIONS
C     TO BE PERFORMED WHILE READING EACH DATA BLOCK INTO IN
C
      INTEGER MAXEDP,MAXEDT,LENEDP,NEDT,IEDT,NPAREDT
      PARAMETER ( MAXEDP=20,MAXEDT=30,LENEDP=40 )
      COMMON /CEDT/ EDTLST(MAXEDP,MAXEDT)
      COMMON /EDT/  NEDT,IEDT,NPAREDT(MAXEDT)
      CHARACTER*(LENEDP) EDTLST
C
C***********************************************************************







